function SelectStartingPlayer()
    StartingPlayer.destruct()
--    local seatedPlayers = getSeatedPlayers()
--    local PlayerHand = Player[seatedPlayers[math.random(#seatedPlayers)]].getPlayerHand()
--    StartingPlayerToken.lock(true)
--    StartingPlayerToken.setPosition({PlayerHand.pos_x + PlayerHand.trigger_forward_x-13, PlayerHand.pos_y, PlayerHand.pos_z})
    local sector1deck = find_pile(Sector1Deck)
    local sector2deck = find_pile(Sector2Deck)
    local sector3deck = find_pile(Sector3Deck)
    sector1deck.shuffle()
    sector2deck.shuffle()
    sector3deck.shuffle()
    ResupplySector1()
    ResupplySector2()
    ResupplySector3()
    --s = ""
--    broadcastToAll(PlayerHand.name, PlayerHand.color)
end
