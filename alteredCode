    PurplePlayerBoard9.getPosition(), PurplePlayerBoard10.getPosition(), PurplePlayerBoard11.getPosition(),
    PurplePlayerBoard12.getPosition()}
    y = 5.00 --starting y position of deployed stacks.  To increase deployment stack size, increase this number
    z = 1.07
    for i=1, 12 do
        GreenBoard[i].y = y
        BlueBoard[i].y = y
        YellowBoard[i].y = y
        RedBoard[i].y = y
        PurpleBoard[i].y = y
        GreenBoard[i].z = GreenBoard[i].z + z
        BlueBoard[i].z = BlueBoard[i].z + z
        YellowBoard[i].z = YellowBoard[i].z + z
        PurpleBoard[i].z = PurpleBoard[i].z + z
        RedBoard[i].z = RedBoard[i].z + z
    end
    math.randomseed(os.time())

    StartingPlayer.createButton({
        click_function = "SelectStartingPlayer",
        function_owner = Global,
        label          = "Start Game",
        position       = {0,-0.25,0},
        rotation       = {0,180,0},
        width          = 500,
        height         = 10,
        font_size      = 80,
        color          = {0, 0, 0},
        font_color     = {1, 1, 1},
    })
